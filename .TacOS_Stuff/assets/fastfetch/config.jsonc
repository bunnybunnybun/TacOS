{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/master/doc/json_schema.json",
  "logo": {
    "type": "auto",
    "source": "/home/carlisle/.config/fastfetch/custom_ascii_art/small_arch.txt",
    "color": {
      "1": "#E40303",
